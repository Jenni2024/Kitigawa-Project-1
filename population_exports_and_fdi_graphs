   "source": [
    "def population(year, sex, age_range, place):\n",
    "    data = pd.read_csv(\"https://github.com/datasets/population/raw/master/data/population.csv\")\n",
    "    \n",
    "    if place == 'WLD':\n",
    "        data = data[data['Location'] == 'World']\n",
    "    elif place == 'COU':\n",
    "        data = data[data['Location Type'] == 'Country']\n",
    "    else:\n",
    "        return \"Invalid place, please choose from 'WLD' or 'COU'\"\n",
    "    \n",
    "    if sex == 'Male':\n",
    "        data = data[data['Sex'] == 'Male']\n",
    "    elif sex == 'Female':\n",
    "        data = data[data['Sex'] == 'Female']\n",
    "    elif sex == 'People':\n",
    "        data = data[data['Sex'] == 'Both sexes']\n",
    "    else:\n",
    "        return \"Invalid sex, please choose from 'Male', 'Female' or 'People'\"\n",
    "    \n",
    "    year_cols = [col for col in data.columns if str(year) in col]\n",
    "    \n",
    "    if not year_cols:\n",
    "        return \"Year not available in the data\"\n",
    "    \n",
    "    data = data[year_cols]\n",
    "    data = data.drop(['Location', 'Location Type', 'Variant', 'Sex'], axis=1)\n",
    "    \n",
    "    age_start, age_end = age_range\n",
    "    data = data[data.columns[(data.columns >= str(age_start)) & (data.columns <= str(age_end))]]\n",
    "    \n",
    "    return data.sum().sum()\n",
    "\n",
    "\n",
    "population(year=2000, sex='Male', age_range=(0, 100), place='WLD')\n"
   ]
  },
